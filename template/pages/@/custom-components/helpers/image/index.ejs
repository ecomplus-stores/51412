<%
  const { width, height } = _.tryImageSize(data.src)
%>

<img 
    <% if (data.lazy == false) { %>
      src="<%= data.src %>"
    <% } else { %>
      class="lozad fade"
      data-src="<%= data.src %>"
    <% } %>
    alt="<%= data.alt %>"
    title="<%= data.alt %>"
    width="<%= width %>"
    height="<%= height %>"
/>