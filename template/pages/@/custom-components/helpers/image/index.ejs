<%
  const { width, height } = _.tryImageSize(data.src)
%>

<img 
    <% if (data.class) { %>
        class="<%= data.class %>"
    <% } %>
    class="lozad fade"
    data-src="<%= data.src %>"
    alt="<%= data.alt %>"
    title="<%= data.alt %>"
    width="<%= width %>"
    height="<%= height %>"
/>