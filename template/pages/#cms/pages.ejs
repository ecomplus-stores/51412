<%
const { slug, content } = await _.resolveRoute()
_.breadcrumbs = [{
  name: content.title,
  link: `/pages/${slug}`
}]

const sections = [
    { type: 'breadcrumbs' },
    { type: 'page-title' },
    { type: 'extra-page' }
]
if (content.sections && content.sections.length) {
  content.sections.forEach(({ type }) => {
    if (type === 'custom-html') {
      sections.push({ type: 'custom-html' })
    }
  });
}

content.sections = sections;

